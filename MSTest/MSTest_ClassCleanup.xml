<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>MSTest_ClassCleanup</Title>
      <Author>Eills</Author>
      <Description>Methods that will be called only once after executing the test methods present in that class.</Description>
      <Shortcut>MSTest_ClassCleanup</Shortcut>
    </Header>
    <Snippet>
      <Code Language="CSharp">
        <![CDATA[
        [ClassCleanup]
        public static void ClassCleanup()
        {
            $Content$
        }]]>
      </Code>
      <Declarations>
        <Literal>
          <ID>Content</ID>
          <ToolTip>Put your clean up flow here.</ToolTip>
          <Default>Console.WriteLine("Inside ClassCleanup");</Default>
        </Literal>
      </Declarations>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>